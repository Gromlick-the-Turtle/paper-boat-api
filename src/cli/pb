import { Command } from 'commander';

import Lookup from '#models/Lookup';
import User from '#models/User';

const cli = new Command();

cli.command(
    'create <model-name>',
    'Create a model and/or related files'
);

cli.command(
    'delete <model-name>',
    'Delete a model and/or related files'
);

cli.command(
    'db',
    'Create sqlite DB with existing models',
);

cli.command(
    'fake <model-name>',
    'Generate a fake object for <model-name>',
);
cli.parse(process.argv)